{
  "name": "lazyvaccine-api",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "DEV_ENV=true nodemon -L index.js",
    "build": "babel src --out-dir build --copy-files --extensions '.ts,.js'",
    "debug": "nodemon --inspect=0.0.0.0:9229 ./index.js",
    "test": "jest --passWithNoTests",
    "test:dev": "docker compose exec nodejs yarn test",
    "test:watch": "jest --passWithNoTests --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier --write",
        "eslint --fix",
        "npm test",
        "git add"
      ]
    },
    "ignore": [
      "**/build/**",
      "**/data/**"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.18.10",
    "@google-cloud/text-to-speech": "^4.0.2",
